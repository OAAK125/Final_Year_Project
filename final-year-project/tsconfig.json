{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",                          
    "paths": {
      "@/*": ["src/*"]                        
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/ui/admin-dashboard/man-cert-top.jsx", "src/components/google/index.tsx", "src/lib/ai.js", "src/app/dashboard/forumthreads/page.jsx", "src/app/admin/users/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}

